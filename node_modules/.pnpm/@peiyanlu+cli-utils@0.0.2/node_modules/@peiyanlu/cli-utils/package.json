{
  "name": "@peiyanlu/cli-utils",
  "version": "0.0.2",
  "description": "Shared utils for building interactive Node.js CLI applications.",
  "license": "MIT",
  "type": "module",
  "private": false,
  "main": "./dist/cjs/index.cjs",
