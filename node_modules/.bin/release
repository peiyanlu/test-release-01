#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/release/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/release/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules:/mnt/e/test-release-01/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/release/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/@peiyanlu+release@file+..+@_ed0402eab1fe9eb8462c1ed2ffff132c/node_modules/@peiyanlu/release/index.js" "$@"
fi
