{
  "name": "@peiyanlu/release",
  "version": "0.0.1",
  "description": "Release project tool.",
  "license": "MIT",
  "type": "module",
  "private": false,
  "bin": {
    "release": "index.js"
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@octokit/rest": "^22.0.1",
    "@peiyanlu/cli-utils": "^0.0.2",
    "@peiyanlu/ts-utils": "^0.0.1",
    "ansis": "^4.2.0",
    "commander": "^14.0.2",
    "mime-types": "^3.0.2",
    "open": "^11.0.0",
    "semver": "^7.7.3",
    "tinyglobby": "^0.2.15"
  },
  "devDependencies": {
    "@types/semver": "^7.7.1",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.10.3",
    "@vitest/coverage-v8": "^4.0.15",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "author": {
    "name": "YanluPei",
    "email": "peiyanlu@vip.qq.com"
  },
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "typescript",
    "cli"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peiyanlu/release.git"
  },
  "bugs": {
    "url": "https://github.com/peiyanlu/release/issues"
  },
  "homepage": "https://github.com/peiyanlu/release#readme",
  "scripts": {
    "dev": "tsdown -w",
    "build": "tsdown",
    "prepublish": "tsdown",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  }
}